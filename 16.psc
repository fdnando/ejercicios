Algoritmo media_aritmetica
	
	dimensión_azarosa = azar(10)
	
	Dimension vector[dimensión_azarosa]
	
	
	
	Repetir
		
		vector[i]=azar(10)
		
		i=i+1
		
	Hasta Que i=dimensión_azarosa
	
	Repetir
		
		Suma=suma+vector[e]
		
		e=e+1
		
	Hasta Que e=dimensión_azarosa
	
	Escribir Suma/dimensión_azarosa
	
	
	
	
	
	
		
	
FinAlgoritmo
